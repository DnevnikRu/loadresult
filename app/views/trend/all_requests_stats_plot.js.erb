var lines = [];

lines.push({
  x: <%= raw(@ids_with_date) %>,
  y: <%= @request_data[:mean] %>,
  name: 'Mean',
  mode: 'lines+markers',
  type: 'scatter',
  yaxis: 'y2'
});
lines.push({
  x: <%= raw(@ids_with_date) %>,
  y: <%= @request_data[:median] %>,
  name: 'Median',
  mode: 'lines+markers',
  type: 'scatter',
  yaxis: 'y2'
});
lines.push({
  x: <%= raw(@ids_with_date) %>,
  y: <%= @request_data[:ninety_percentile] %>,
  name: '90%',
  mode: 'lines+markers',
  type: 'scatter',
  yaxis: 'y2'
});
lines.push({
  x: <%= raw(@ids_with_date) %>,
  y: <%= @request_data[:throughput] %>,
  name: 'Throughput',
  mode: 'lines+markers',
  type: 'scatter'
});

var layout = {
  xaxis: {
    tickangle: 30,
    showticklabels: <%= @ids_with_date.count < 20 %>
  },
  yaxis2: {
    title: 'ms',
    domain: [0.2, 1]
  },
  yaxis: {
    title: 'req/sec',
    domain: [0, 0.2]
  },
};

Plotly.newPlot('all_requests_stats_plot', lines, layout);
