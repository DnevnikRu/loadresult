$('#percentile_requests_button').prop('type', 'button');

var one_to_100 = [];
for (var i = 1; i <= 100; i++) {
  one_to_100.push(i);
}

var result1 = {
  x: one_to_100,
  y: <%= @result1_data %>,
  name: '<%= params[:version1]%> (<%= params[:test_run_date1]%>)',
  mode: 'lines',
  line: {shape: 'spline'},
  type: 'scatter'
};

var result2 = {
  x: one_to_100,
  y: <%= @result2_data %>,
  name: '<%= params[:version2]%> (<%= params[:test_run_date2]%>)',
  mode: 'lines',
  line: {shape: 'spline'},
  type: 'scatter'
};
var layout = {
  title: 'Percentile of requests',
  xaxis: {
    title: 'Percentile'
  },
  yaxis: {
    title: 'Response time, ms'
  },
  legend: {
    orientation: 'h',
    x: 0.3,
    y: -0.3
  }
};
Plotly.newPlot('percentile_requests_plot', [result1, result2], layout);