$('#percentile_requests_button').prop('type', 'button');

var result = {
    y: <%= @result_data %>,
    name: 'id:<%= params[:result_id] %>',
    mode: 'lines',
    line: {shape: 'spline'},
    type: 'scatter'
};

var layout = {
    title: 'Percentile of requests',
    xaxis: {
        title: 'Percentile'
    },
    yaxis: {
        title: 'Response time, ms'
    }
};
Plotly.newPlot('percentile_requests_plot', [result], layout);