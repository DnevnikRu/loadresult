$('#<%= "#{@plot_id}_button" %>').prop('type', 'button');

var result = {
  x: <%= @result_data[:values] %>,
  y: <%= @result_data[:percentiles] %>,
  name: 'id:<%= params[:result_id] %>',
  mode: 'lines',
  line: {shape: 'spline'},
  type: 'scatter'
};

var layout = {
  title: 'Response time percentiles',
  xaxis: {
    title: 'Percentile'
  },
  yaxis: {
    title: 'Response time, ms'
  }
};
Plotly.newPlot('<%= "#{@plot_id}_div" %>', [result], layout);