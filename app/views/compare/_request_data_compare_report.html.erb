<div class="panel-group" id="requests_data">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h4 class="panel-title">
        <a class="accordion-toggle" data-toggle="collapse" data-parent="#requestsData" href="#collapseRequestsData">
          Requests Data
        </a>
      </h4>
    </div>
    <div id="collapseRequestsData" class="panel-collapse collapse">
      <div class="panel-body">
        <div class="panel-group" id="params">
          <div class="panel panel-default">
            <div class="panel-heading">
              <h4 class="panel-title">
                <a class="accordion-toggle" data-toggle="collapse" data-parent="#params" href="#collapseParams">
                  Parameters
                </a>
              </h4>
            </div>
            <div id="collapseParams" class="panel-collapse collapse">
              <div class="panel-body">
                <div class="row">
                  <div class="col-md-6">
                    <div class="input-group">
                      <span class="input-group-addon">Show columns</span>

                      <div class="input-group-btn btn-group-custom" data-toggle="buttons">
                        <label class="btn btn-primary active">
                          <input id="btn_show_mean" type="checkbox" checked onchange="showColumn('btn_show_mean', 'mean');">
                          Mean
                        </label>
                        <label class="btn btn-primary active">
                          <input id="btn_show_median" type="checkbox" checked onchange="showColumn('btn_show_median', 'median');">
                          Median
                        </label>
                        <label class="btn btn-primary active">
                          <input id="btn_show_90line" type="checkbox" checked onchange="showColumn('btn_show_90line', 'line90');">
                          90% Line
                        </label>
                        <label class="btn btn-primary">
                          <input id="btn_show_min" type="checkbox" onchange="showColumn('btn_show_min', 'min');">
                          Min
                        </label>
                        <label class="btn btn-primary">
                          <input id="btn_show_max" type="checkbox" onchange="showColumn('btn_show_max', 'max');">
                          Max
                        </label>
                        <label class="btn btn-primary">
                          <input id="btn_show_throughput" type="checkbox" onchange="showColumn('btn_show_throughput', 'throughput');">
                          Throughput
                        </label>
                        <label class="btn btn-primary">
                          <input id="btn_show_failed_req" type="checkbox" onchange="showColumn('btn_show_failed_req', 'failed_request');">
                          Failed requests
                        </label>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <br>
        <table class="table table-hover table-striped table-condensed table-responsive table-bordered" id="requests-results">
          <thead>
          <tr>
            <th></th>
            <th class="mean">Mean first, msec</th>
            <th class="mean">Mean second, msec</th>
            <th class="mean">Mean trend, msec</th>
            <th class="median">Median first, msec</th>
            <th class="median">Median second, msec</th>
            <th class="median">Median trend, msec</th>
            <th class="line90">90% Line first, msec</th>
            <th class="line90">90% Line second, msec</th>
            <th class="line90">90% Line trend, msec</th>
            <th class="min">Min first, msec</th>
            <th class="min">Min second, msec</th>
            <th class="max">Max first, msec</th>
            <th class="max">Max second, msec</th>
            <th class="throughput">Throughput first, req/sec</th>
            <th class="throughput">Throughput second, req/sec</th>
            <th class="failed_request">Failed requests first, %</th>
            <th class="failed_request">Failed requests second, %</th>
            <th class="failed_request">Failed requests trend, %</th>
          </tr>
          </thead>
          <tbody>
          <tr>
            <td class="hidden-row"></td>
          </tr>
          <% @compare_report.request_labels.each do |label| %>
              <% label_uniq_id = SecureRandom.hex %>
              <tr>
                <td>
                  <%= button_to label,
                                compare_requests_histogram_plot_path(
                                    label: label,
                                    result1_id: @compare_report.result1.id,
                                    result2_id: @compare_report.result2.id,
                                    plot_id: label_uniq_id
                                ),
                                remote: true,
                                id: "#{label_uniq_id}_button",
                                class: 'btn btn-default show-plot',
                                data: { disable_with: 'Loading..' },
                                'data-toggle' => 'collapse',
                                'data-target' => "##{label_uniq_id}_div" %>
                </td>
                <td class="mean"><%= @compare_report.result1.calculated_requests_results.find_by(label: label).mean %></td>
                <td class="mean"><%= @compare_report.result2.calculated_requests_results.find_by(label: label).mean %></td>
                <td class="mean trend"><%= @compare_report.trend(:calculated_requests_results, :mean, label) %> </td>
                <td class="median"><%= @compare_report.result1.calculated_requests_results.find_by(label: label).median %></td>
                <td class="median"><%= @compare_report.result2.calculated_requests_results.find_by(label: label).median %></td>
                <td class="median trend"><%= @compare_report.trend(:calculated_requests_results, :median, label) %> </td>
                <td class="line90"><%= @compare_report.result1.calculated_requests_results.find_by(label: label).ninety_percentile %></td>
                <td class="line90"><%= @compare_report.result2.calculated_requests_results.find_by(label: label).ninety_percentile %></td>
                <td class="line90 trend"><%= @compare_report.trend(:calculated_requests_results, :ninety_percentile, label) %> </td>
                <td class="min"><%= @compare_report.result1.calculated_requests_results.find_by(label: label).min %></td>
                <td class="min"><%= @compare_report.result2.calculated_requests_results.find_by(label: label).min %></td>
                <td class="max"><%= @compare_report.result1.calculated_requests_results.find_by(label: label).max %></td>
                <td class="max"><%= @compare_report.result2.calculated_requests_results.find_by(label: label).max %></td>
                <td class="throughput"><%= @compare_report.result1.calculated_requests_results.find_by(label: label).throughput %></td>
                <td class="throughput"><%= @compare_report.result2.calculated_requests_results.find_by(label: label).throughput %></td>
                <td class="failed_request"><%= @compare_report.result1.calculated_requests_results.find_by(label: label).failed_results %></td>
                <td class="failed_request"><%= @compare_report.result2.calculated_requests_results.find_by(label: label).failed_results %></td>
                <td class="failed_request trend"><%= @compare_report.trend(:calculated_requests_results, :failed_results, label) %></td>
              </tr>
              </tr>
              <tr>
                <td colspan="100" class="hidden-row">
                  <div class="collapse" id="<%= "#{label_uniq_id}_div" %>"></div>
                </td>
              </tr>
          <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>