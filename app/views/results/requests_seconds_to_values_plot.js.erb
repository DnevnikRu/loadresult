$('#<%= "#{@plot_id}_button" %>').prop('type', 'button');

var result = {
    x: <%= @result_data[:seconds] %>,
    y: <%= @result_data[:values] %>,
    opacity: 0.75,
    name: 'id:<%= params[:result_id] %>',
    mode: 'lines'
};

var layout = {
    title: 'Response time to seconds',
    xaxis: {
        title: 'Time, seconds'
    },
    yaxis: {
        title: 'Response time, ms'
    }
};
Plotly.newPlot('<%= "#{@plot_id}_div" %>', [result], layout);
