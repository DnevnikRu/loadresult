$('#percentile_requests_button').prop('type', 'button');

var result1 = {
    y: <%= @result1_data %>,
    name: 'id:<%= params[:result1_id] %>',
    mode: 'lines',
    line: {shape: 'spline'},
    type: 'scatter'
};
var result2 = {
    y: <%= @result2_data %>,
    name: 'id:<%= params[:result2_id] %>',
    mode: 'lines',
    line: {shape: 'spline'},
    type: 'scatter'
};
var layout = {
    title: 'Percentile of requests',
    xaxis: {
        title: 'Percentile'
    },
    yaxis: {
        title: 'Response time, ms'
    }
};
Plotly.newPlot('percentile_requests_plot', [result1, result2], layout);