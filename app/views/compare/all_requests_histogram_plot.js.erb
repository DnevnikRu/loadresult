$('#all_requests_plot_button').prop('type', 'button');

var result1 = {
    x: <%= @result1_data %>,
    opacity: 0.75,
    name: 'id:<%= params[:result1_id] %>',
    type: 'histogram'
};
var result2 = {
    x: <%= @result2_data %>,
    opacity: 0.75,
    name: 'id:<%= params[:result2_id] %>',
    type: 'histogram'
};
var layout = {
    title: 'Histogram of response time',
    xaxis: {
        title: 'Response time, ms'
    },
    yaxis: {
        title: 'Number of requests'
    },
    barmode: 'overlay'
};
Plotly.newPlot('all_requests_plot', [result1, result2], layout);