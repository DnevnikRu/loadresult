$('#<%= "#{@plot_id}_button" %>').prop('type', 'button');


var result1 = {
    x: <%= @result1_data[:values] %>,
    y: <%= @result1_data[:percentiles] %>,
    name: '<%= params[:version1]%> (<%= params[:test_run_date1]%>)',
    mode: 'lines',
    line: {shape: 'spline'},
    type: 'scatter'
};

var result2 = {
    x: <%= @result2_data[:values] %>,
    y: <%= @result2_data[:percentiles] %>,
    name: '<%= params[:version2]%> (<%= params[:test_run_date2]%>)',
    mode: 'lines',
    line: {shape: 'spline'},
    type: 'scatter'
};
var layout = {
    title: 'Response time percentiles',
    xaxis: {
        title: 'Response time, ms'
    },
    yaxis: {
        title: 'Percentile'
    },
    legend: {
        orientation: 'h',
        x: 0.3,
        y: -0.3
    }
};
Plotly.newPlot('<%= "#{@plot_id}_div" %>', [result1, result2], layout);