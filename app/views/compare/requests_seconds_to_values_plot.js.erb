$('#<%= "#{@plot_id}_button" %>').prop('type', 'button');

var result1 = {
  x: <%= @result1_data[:seconds] %>,
  y: <%= @result1_data[:values] %>,
  opacity: 0.75,
  name: '<%= params[:version1]%> (<%= params[:test_run_date1]%>)',
  mode: 'lines'
};
var result2 = {
  x: <%= @result2_data[:seconds] %>,
  y: <%= @result2_data[:values] %>,
  opacity: 0.75,
  name: '<%= params[:version2]%> (<%= params[:test_run_date2]%>)',
  mode: 'lines'
};
var layout = {
  title: 'Response time to seconds',
  xaxis: {
    title: 'Time, seconds'
  },
  yaxis: {
    title: 'Response time, ms'
  },
  legend: {
    orientation: 'h',
    x: 0.3,
    y: -0.3
  }
};
Plotly.newPlot('<%= "#{@plot_id}_div" %>', [result1, result2], layout);
