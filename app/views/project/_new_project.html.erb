<h4>New project</h4>
<%= form_tag({action: "create"}, multipart: true) do %>
    <div class="form-group required">
      <%= label_tag :project_name, nil, class: "control-label" %>
      <%= text_field_tag :project_name, flash[:project_name], class: "form-control", placeholder: 'rotator' %>
    </div>
    <%= button_tag "Save", class: "btn btn-primary", 'data-disable-with' => 'Loading..' %>
<% end %>
